<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
        "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="mockLibrary" class="org.robotframework.jvmconnector.mocks.MockJavaLibrary" />
										
	<bean id="rmiService" class="org.robotframework.jvmconnector.server.SimpleRobotRmiService">
		<property name="library" ref="mockLibrary" />
	</bean>

	<bean id="serviceExporter" class="org.robotframework.jvmconnector.util.MyRmiServiceExporter">
		<property name="serviceName" value="jvmConnector" />
		<property name="service" ref="rmiService" />
		<property name="registryPort" value="1099" />
		<property name="serviceInterface" value="org.robotframework.jvmconnector.server.RobotRmiService" />
	</bean>
</beans>
